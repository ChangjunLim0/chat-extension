{
    "manifest_version": 3,
    "name": "chat AI extension",
    "description": "Extension for improving chat AI services",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [
                "https://gemini.google.com/*"
            ],
            "js": [
                "scripts/gemini.js"
            ],
            "css": ["styles.css"]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [ "fonts/*.woff2" ],
            "matches": [ "https://gemini.google.com/*" ]
        }
    ]
}